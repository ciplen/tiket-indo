<?xml version="1.0"?>
<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<deployment>
		<exclusions>
		</exclusions>
		<dependencies>
			<!--Use libraries provided by wildfly. This way we can exclude the library from war file-->
